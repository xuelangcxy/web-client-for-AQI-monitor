<div id="page" style="width: 800px; height: 520px; text-align: center; background-color: #F8F8F8">
    <div class="register">
        <form>
            <fieldset id="register">
                <legend>
                    <h2 class="form-signin-heading">用户注册</h2>
                </legend>

                <label for="inputUserName" class="sr-only">用户名</label>
                <p>
                    用户名：
                    <input type="userName" id="userName" class="form-control" placeholder="用户名" required autofocus></p>
                <label for="inputEmail" class="sr-only">邮箱地址</label>
                <p>
                    邮箱：
                    <input type="email" id="email" class="form-control" placeholder="邮箱地址"></p>
                <label for="inputPassword" class="sr-only">密码</label>
                <p>
                    密码：
                    <input type="password" id="password" class="form-control" placeholder="密码" required></p>

                <input id="submits" type="button" value="提交注册信息" class="btn btn-lg btn-primary btn-block">

                <div class="row">
                    <div class="col-lg-8" style="margin-top: 5%; align: left">
                        <h4>
                            已有账号，直接
                            <a href="login" style="color: blue;"> <strong>登录</strong>
                            </a>
                        </h4>
                    </div>
                    <div class="col-lg-4" style="margin-top: 2%;">
                        <div align="right">
                            <input id="reset" type="button" value="重置" class="btn btn-primary" style="margin-top:3%;"></div>
                    </div>
                </div>
            </fieldset>
        </form>
    </div>
</div>

<script src="javascripts/socketio.js"></script>
<script src="javascripts/socket.io-stream.js"></script>
<script src="javascripts/blob-stream.js"></script>
<script src="javascripts/mimonodejsSDK.js"></script>
